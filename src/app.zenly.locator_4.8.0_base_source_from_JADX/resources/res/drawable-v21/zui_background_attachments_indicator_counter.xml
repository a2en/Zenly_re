<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/zui_color_white_100"/>
        </shape>
    </item>
    <item android:id="@+id/inner_circle" android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="7dp"/>
            <solid android:color="@color/zui_color_primary"/>
        </shape>
    </item>
</layer-list>
